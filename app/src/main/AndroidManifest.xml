<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hbcx.user">
    <!-- 极光 -->
    <!-- Required -->
    <permission
        android:name="${JPUSH_PKGNAME}.permission.JPUSH_MESSAGE"
        android:protectionLevel="signature" />

    <!-- 极光 Required -->
    <uses-permission android:name="${JPUSH_PKGNAME}.permission.JPUSH_MESSAGE" />
    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <application
        android:name=".YyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme">
        <service
            android:name="cn.jpush.android.service.PushService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTER" />
                <action android:name="cn.jpush.android.intent.REPORT" />
                <action android:name="cn.jpush.android.intent.PushService" />
                <action android:name="cn.jpush.android.intent.PUSH_TIME" />
            </intent-filter>
        </service>
        <!-- Required -->
        <receiver
            android:name="cn.jpush.android.service.PushReceiver"
            android:enabled="true">
            <intent-filter android:priority="1000">
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED_PROXY" />

                <category android:name="${JPUSH_PKGNAME}" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <!-- Optional -->
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>
        <!-- Required SDK核心功能 -->
        <activity
            android:name="cn.jpush.android.ui.PushActivity"
            android:configChanges="orientation|keyboardHidden"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.ui.PushActivity" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="${JPUSH_PKGNAME}" />
            </intent-filter>
        </activity>
        <!-- Required SDK核心功能 -->
        <service
            android:name="cn.jpush.android.service.DownloadService"
            android:enabled="true"
            android:exported="false" />
        <!-- Required SDK核心功能 -->
        <receiver android:name="cn.jpush.android.service.AlarmReceiver" />
        <!-- User defined. 用户自定义的广播接收器 -->
        <receiver
            android:name=".jpush.PushReceiver"
            android:enabled="true">
            <intent-filter>

                <!-- Required 用户注册SDK的intent -->
                <action android:name="cn.jpush.android.intent.REGISTRATION" />
                <!-- Required 用户接收SDK消息的intent -->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" />
                <!-- Required 用户接收SDK通知栏信息的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" />
                <!-- Required 用户打开自定义通知栏的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" />
                <!-- Optional 用户接受Rich Push Javascript 回调函数的intent -->
                <action android:name="cn.jpush.android.intent.ACTION_RICHPUSH_CALLBACK" />
                <!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <action android:name="cn.jpush.android.intent.CONNECTION" />

                <category android:name="${JPUSH_PKGNAME}" />
            </intent-filter>
        </receiver>
        <!-- Required. For publish channel feature -->
        <!-- JPUSH_CHANNEL 是为了方便开发者统计APK分发渠道。 -->
        <!-- 例如: -->
        <!-- 发到 Google Play 的APK可以设置为 google-play; -->
        <!-- 发到其他市场的 APK 可以设置为 xxx-market。 -->
        <!-- 目前这个渠道统计功能的报表还未开放。 -->
        <meta-data
            android:name="JPUSH_CHANNEL"
            android:value="developer-default" />
        <!-- Required. AppKey copied from Portal -->
        <meta-data
            android:name="JPUSH_APPKEY"
            android:value="${JPUSH_APPKEY}" />

        <activity android:name=".ui.MainActivity" android:launchMode="singleTask">

        </activity>
        <activity android:name=".ui.SplashActivity" android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.trip.SearchAddressActivity" />
        <activity android:name=".ui.trip.TripActivity" />
        <activity android:name=".ui.trip.OrderDetailActivity" />
        <activity android:name=".ui.trip.ComplainActivity" />
        <activity android:name=".ui.trip.DriverInfoActivity" />
        <activity android:name=".ui.trip.CancelOrderActivity" />
        <activity android:name=".ui.order.MyTripActivity" />
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity android:name=".ui.H5Activity" />
        <activity android:name=".ui.trip.PriceDetailActivity" />
        <activity android:name=".ui.user.MoreActivity" />
        <activity android:name=".ui.user.SafetyActivity" />
        <activity android:name=".ui.user.ChangePhoneActivity" />
        <activity android:name=".ui.user.VerifyPhoneActivity" />
        <activity android:name=".ui.rent.SelectAddressActivity" />
        <activity android:name=".ui.rent.SearchRentPlaceActivity" />
        <activity android:name=".ui.rent.CarListActivity" />
        <activity android:name=".ui.rent.CarCompanyListActivity" />
        <activity android:name=".ui.rent.OrderConfirmActivity" />
        <activity android:name=".ui.rent.CompanyActivity" />
        <activity android:name=".ui.rent.CompanyPointActivity" />
        <activity android:name=".ui.rent.SelectDriverActivity" />
        <activity android:name=".ui.rent.AddDriverActivity" />
        <activity android:name=".ui.user.CouponActivity" />
        <activity android:name=".ui.rent.RentSafeActivity" />
        <activity android:name=".ui.PayActivity" />
        <activity android:name=".ui.PaySuccessActivity" >

        </activity>
        <activity android:name=".ui.rent.RentOrderDetailActivity" />
        <activity android:name=".ui.grouprent.CompanyListActivity" />
        <activity android:name=".ui.SelectCityActivity"/>
        <activity android:name=".ui.grouprent.MoreCarListActivity" />
        <activity android:name=".ui.grouprent.GroupOrderConfirmActivity" />
        <activity android:name=".ui.grouprent.GroupRentOrderDetailActivity" />
        <activity android:name=".ui.ticket.CalendarActivity" />
        <activity android:name=".ui.ticket.TicketListActivity" />
        <activity android:name=".ui.ticket.LineDetailActivity" />
        <activity android:name=".ui.ticket.TicketOrderConfirmActivity" >
        </activity>
        <activity android:name=".ui.ticket.AddPassengerActivity" />
        <activity android:name=".ui.ticket.TicketOrderDetailActivity" />
        <activity android:name=".ui.ticket.CancelTicketActivity" />
        <activity android:name=".ui.ticket.LineListActivity" />
        <activity android:name=".ui.ticket.TicketPaySuccessActivity" />
        <activity android:name=".ui.user.EditActivity" />
        <activity android:name=".ui.ticket.EvaluateActivity" android:windowSoftInputMode="stateHidden|adjustResize"/>
        <activity android:name=".ui.user.MyCouponActivity" />
        <activity android:name=".ui.user.MyScoreActivity" />
        <activity android:name=".ui.user.InviteActivity" />
        <activity android:name=".ui.user.InviteRecordActivity" />
        <activity android:name=".ui.user.FeedbackActivity" />
        <activity android:name=".ui.user.MessageActivity" />
        <activity android:name=".ui.grouprent.SignActivity" />

        <activity
            android:name=".wxapi.WXPayEntryActivity"
            android:exported="true" />
        <activity
            android:name=".wxapi.WXEntryActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:exported="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".ui.ticket.ChooseStationActivity" />
        <activity android:name=".ui.ticket.SearchStationActivity" />
    </application>
</manifest>